{
    "name": "lukasz-adamski/laravel-cashbill",
    "description": "Laravel package which provides CashBill payment processor support.",
    "keywords": ["laravel", "cashbill", "framework", "source", "api"],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Lukasz Adamski"
        }
    ],
    "require": {
        "illuminate/support": "^6.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Adams\\CashBill\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adams\\CashBill\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    }
}